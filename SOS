int x;
void setup() {
Serial.begin(9600);
  
  pinMode(13, OUTPUT);
  pinMode(7, INPUT);
}

void loop() {
 for(int x=0; x<=2;x++){
  digitalWrite(13,HIGH);
  delay(500);
  digitalWrite(13,LOW);
  delay(500);
 }
  for(int x=0; x<=2;x++){
  digitalWrite(13,HIGH);
  delay(1200);
  digitalWrite(13,LOW);
  delay(1200);
  }
   for(int x=0; x<=2;x++){
  digitalWrite(13,HIGH);
  delay(500);
  digitalWrite(13,LOW);
  delay(500);
   }
 }
